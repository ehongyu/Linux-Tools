alias mv='mv -i'
alias cp='cp -i'
alias rm='rm -i'
alias loc='find . -name'
alias s=source
alias ls='ls -G'
alias l='ls -ltG'
alias new='ls -lt | head -20'
alias grep='grep -i'
alias his='history | tail -20'
alias diff='diff -W $(( $(tput cols) - 2 ))'
alias pretty_json=jq
alias sumup="awk '{s+=\$1} END {print s}'"
alias myps='ps -ef | grep $(whoami)'
alias clean_python_cache='find . -name "*.pyc" -exec rm -f {} \;'
alias j8='export JAVA_HOME=`/usr/libexec/java_home -v 1.8`'
alias j12='export JAVA_HOME=`/usr/libexec/java_home -v 12`'
